<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
								           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
		{#{{ encore_entry_link_tags('app') }}#}
		<!-- Axentix CSS minified version -->
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/css/axentix.min.css">
		<!-- Bootstrap 5 CSS minified version -->
		<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
		<link rel="stylesheet" href="{{ asset('css/styles.css') }}">

		

		
		{% endblock %}
	</head>
	<body>
		<header>
			<nav class="navbar shadow-1 primary">
				<a href="{{ path('Ma_page_accueil') }}" class="navbar-brand">Petites Annonces</a>
				<div class="navbar-menu ml-auto">
					<a class="navbar-link" href="#">Accueil</a>
					<a class="navbar-link" href="#">Link 2</a>
					<a class="navbar-link" href="#">Link 3</a>
					<a class="navbar-link" href="#">Link 4</a>
				</div>
			</nav>
			<section class="container">
				<div class="row">
					<div class="col-12">
					</div>
				</div>
			</section>
		</header>

		<main>
			{% block body %}{% endblock %}
		</main>

		<footer class="footer primary">
			Copyright © 2021 - Sébastien CAZI
		</footer>

{% block javascripts %}
	{#{{ encore_entry_script_tags('app') }}#}
	<!-- Bootstrap 5 + jQuery + Popper JS minified version -->
	<!-- Axentix JS minified version -->
<script src="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/js/axentix.min.js"></script>
<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>

<script src="{{ asset('js/scripts.js') }}"></script>
{% endblock %}

	</body>
</html>
